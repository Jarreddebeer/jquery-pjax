{
    "name" : "jquery-pjax",
    "description": "pjax is a jQuery plugin that uses ajax and pushState to deliver a fast browsing experience with real permalinks, page titles, and a working back button.",
    "homepage": "https://github.com/defunkt/jquery-pjax",
    "author": "Chris Wanstrath",
    "repository": {"type": "git", "url": "git://github.com/defunkt/jquery-pjax.git"},
    "main": "jquery.pjax.js",
    "version": "1.7.3",
    "jspm": {
        "main": "jquery.pjax",
        "files": ["jquery.pjax.js"],
        "shim": {
            "jquery.pjax": {
                "imports": ["jquery"],
                "exports": "$"
            }
        },
        "buildConfig": {
            "uglify": true
        }
    }
}
